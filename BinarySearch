def binarysearch(list1,key): low = 0
high = len(list1)-1 found = False
while low<=high and not found: mid = (low+high)//2
if key == list1[mid]: found = True

elif key>list1[low]: low = mid+1
else:
high = mid-1 if found == True:
print('key is found at index',mid) else:
print('key is not found')

list1=[1,2,3,4,5]
list1.sort() binarysearch(list1,4)
